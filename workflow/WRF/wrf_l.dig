timezone: UTC

schedule:
  cron>: 0 16 * * *

sla:
  duration: 05:30:00
  fail: true
  +notice:
    +clean_up:
      sh>: /home/weather/tools/pre_WRF/remove_gfs.py GFS_l -c ${moment(session_time).format("YYYYMMDDHHmm")}
    +kill_task:
      sh>: /home/weather/tools/digdag/kill_attempt.sh ${session_id}

+check_files:
  sh>: /home/weather/tools/pre_WRF/file_checker.py GFS_l -c ${moment(session_time).format("YYYYMMDDHHmm")}

+sync_gfs: 
  sh>: /home/weather/tools/pre_WRF/sync_gfs.sh

+remove_gfs:
  sh>: /home/weather/tools/pre_WRF/remove_gfs.py GFS_l -c ${moment(session_time).format("YYYYMMDDHHmm")}

